{
  "manifest_version": 2,
  "name": "Auto Login for Testsystems KLP Swiss Post",
  "description": "auto login functionality for email & pw and/or sms code",
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "version": "1.1.0.0",
  "author": "Stef KÃ¤ser",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  },
  "content_scripts": [
    {
      "matches": [
        "https://accountint1.post.ch/idp/*",
        "https://accountint2.post.ch/idp/*"
      ],
      "js": [
        "contentscript.js"
      ]
    }
  ]
}